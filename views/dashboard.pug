extends layout

block css
    link(rel='stylesheet', href='/stylesheets/dashboard-418335c17d9834b2d43d4520b74ce0ec.css')

block content
    div#dashboard-drawer.mdl-layout__drawer
        span.mdl-layout-title #{user.name}
        nav.mdl-navigation
            a.mdl-navigation__link.mdl-typography--text-uppercase(href="link") Link
    div#page-content
        table.mdl-data-table.mdl-js-data-table.mdl-data-table--selectable.mdl-shadow--2dp
            thead
                tr
                    th.mdl-data-table__cell---non-numeric Material
                    th Quantity
                    th Unit price
            tbody
                tr
                    td.mdl-data-table__cell--non-numberic Contentname
                    td how many
                    td how much







