extends layout

block css
    link(rel='stylesheet', href='/stylesheets/dashboard-c57e96675273b44829b25fe77e3744f7.css')

block content
    div.mdl-layout.mdl-js-layout.mdl-layout--fixed-drawer
        div.mdl-layout__drawer
            span.mdl-layout-title #{user.name}
            nav.mdl-navigation
            a.mdl-navigation__link(href="link") Link
    main.mdl-layout__content
        div.page-content

            table.mdl-data-table.mdl-js-data-table.mdl-data-table--selectable.mdl-shadow--2dp
                thead
                    tr
                        th.mdl-data-table__cell---non-numeric Material
                        th Quantity
                        th Unit price
                tbody
                    tr
                        td.mdl-data-table__cell--non-numberic Contentname
                        td how many
                        td how much







