extends layout

block css
    link(rel='stylesheet', href='/stylesheets/dashboard-38a269c4497a68118fb3038073180422.css')

block content
    div#dashboard-drawer.mdl-layout__drawer.mdl-color--grey-100
        div.mdl-layout-title.mdl-color--grey-200.mdl-color-text--grey-800 #{user.name}
        nav.mdl-navigation
            a.mdl-navigation__link.mdl-color-text--grey-800(href="")
                i.material-icons.mdl-color-text--grey-600 home
                | Home
    div#page-content
        table.mdl-data-table.mdl-js-data-table.mdl-data-table--selectable.mdl-shadow--2dp
            thead
                tr
                    th.mdl-data-table__cell---non-numeric Material
                    th Quantity
                    th Unit price
            tbody
                tr
                    td.mdl-data-table__cell--non-numberic Contentname
                    td how many
                    td how much







