{
  "name": "ctp-project",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "dev:nodemon": "nodemon ./bin/www --ignore src/ --ignore tests/ --ignore assets/ --ignore webpack.config.js --ignore models/ --ignore assets/",
    "dev:nodemon:poll": "nodemon ./bin/www -L --ignore src/ --ignore tests/ --ignore assets/ --ignore webpack.config.js --ignore models/ --ignore assets/",
    "dev:webpack": "webpack --watch",
    "dev:webpack:poll": "webpack --watch --watch-poll",
    "dev:poll": "npm-run-all --parallel dev:nodemon:poll dev:webpack:poll",
    "dev": "npm-run-all --parallel dev:nodemon dev:webpack",
    "build": "webpack",
    "test": "mocha ./bin/test",
    "migrate": "sequelize db:migrate:undo:all && sequelize db:migrate"
  },
  "dependencies": {
    "body-parser": "^1.15.2",
    "compression": "^1.6.2",
    "connect-ensure-login": "^0.1.1",
    "connect-session-sequelize": "^4.0.0",
    "cookie-parser": "^1.4.3",
    "cookie-session": "^2.0.0-alpha.1",
    "debug": "^2.2.0",
    "express": "^4.14.0",
    "express-session": "^1.14.1",
    "morgan": "^1.7.0",
    "passport": "^0.3.2",
    "passport-auth0": "^0.5.2",
    "pg": "^6.1.0",
    "pug": "^2.0.0-beta6",
    "request": "^2.75.0",
    "sequelize": "^3.24.3",
    "serve-favicon": "^2.3.0"
  },
  "devDependencies": {
    "babel-core": "^6.18.0",
    "babel-loader": "^6.2.7",
    "babel-preset-es2015": "^6.18.0",
    "chai": "^3.5.0",
    "chai-http": "^3.0.0",
    "css-loader": "^0.25.0",
    "mocha": "^3.1.0",
    "node-sass": "^3.10.1",
    "npm-run-all": "^3.1.1",
    "pug-loader": "^2.3.0",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.0.2",
    "sequelize-cli": "^2.4.0",
    "style-loader": "^0.13.1",
    "vue": "^2.0.3",
    "vue-material": "^0.2.0",
    "vue-resource": "^1.0.3",
    "vue-router": "^2.0.1",
    "vuex": "^2.0.0",
    "webpack": "^2.1.0-beta.25"
  }
}
